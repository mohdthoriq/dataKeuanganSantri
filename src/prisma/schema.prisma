generator client {
  provider = "prisma-client-js"
  output   = "../generated"
}

datasource db {
  provider = "postgresql"
}

enum user_role {
  ADMIN
  WALI_SANTRI
}

enum category_type {
  PEMASUKAN
  PENGELUARAN
}

enum PaymentStatus {
  PENDING
  PAID
  FAILED
  EXPIRED
}

enum InvoiceStatus {
  UNPAID
  PARTIAL
  PAID
  EXPIRED
}
