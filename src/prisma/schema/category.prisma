model Category {
  id            Int           @id @default(autoincrement())
  name          String
  type          category_type
  institutionId Int
  isActive      Boolean       @default(true)
  createdAt     DateTime      @default(now())
  institution   Institution   @relation(fields: [institutionId], references: [id])
  transactions  Transaction[]

  @@map("category")
}
