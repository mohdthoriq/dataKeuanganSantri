model AuthAccount {
  id        Int      @id @default(autoincrement())
  username  String
  password  String
  userId    Int
  santriId  Int?
  isActive  Boolean  @default(true)
  createdAt DateTime @default(now())

  // relations
  user   User   @relation(fields: [userId], references: [id])
  santri Santri? @relation(fields: [santriId], references: [id])

  @@map("auth_account")
}