model auth_account {
  id        Int      @id @default(autoincrement())
  username  String
  password  String
  userId    Int
  santriId  Int?
  isActive  Boolean  @default(true)
  createdAt DateTime @default(now())
  santri    santri?  @relation(fields: [santriId], references: [id])
  user      user     @relation(fields: [userId], references: [id])

  @@map("auth_account")
}
