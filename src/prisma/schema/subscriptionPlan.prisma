model SubscriptionPlan {
  id                String             @id @default(uuid())
  name              String
  price             Decimal
  interval          PlanInterval
  description       String?
  createdAt         DateTime           @default(now())
  invoices          Invoice[]
  userSubscriptions UserSubscription[]

  isActive    Boolean  @default(true) 

  @@map("subscription_plan")
}
