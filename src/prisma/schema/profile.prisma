model Profile {
  id                  Int     @id @default(autoincrement())
  name                String
  gender              String?
  address             String
  profile_picture_url String?
  public_id           String?
  userId              Int     @unique
  occupation          String?
  phone               String?
  user                Users    @relation(fields: [userId], references: [id])

  @@map("profile")
}
