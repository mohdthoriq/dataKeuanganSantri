model Profile {
  id                  String  @id @default(uuid())
  name                String
  gender              String?
  address             String
  profile_picture_url String?
  public_id           String?
  userId              String  @unique
  occupation          String?
  phone               String?
  user                Users   @relation(fields: [userId], references: [id])

  @@map("profile")
}
